{
    "version": 2,
    "builds": [
        {
            "src": "StudAIs/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "buildCommand": "apt-get update && apt-get install -y default-libmysqlclient-dev build-essential pkg-config && python manage.py migrate"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "StudAIs/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "StudAIs.settings",
        "PYTHONPATH": ".",
        "ALLOWED_HOSTS": ".vercel.app",
        "DEBUG": "False"
    }
}