{
    "version": 2,
    "builds": [
        {
            "src": "StudAIs/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "buildCommand": "python manage.py migrate"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "StudAIs/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "StudAIs.settings",
        "PYTHONPATH": ".",
        "ALLOWED_HOSTS": ".vercel.app",
        "DEBUG": "False"
    }
}